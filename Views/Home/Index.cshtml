@{
    ViewData["Title"] = "Home Page";
}

<style>
    .fs{
        display:none;
    }
</style>
<div class="text-center">
    <h1 class="display-4">Welcome</h1>
    <p>Honeypot Bot detection</p>
</div>
<form action="/Home/Register" method="post">
    <input type="text" name="Id" class="form-control fs"/>
    <label>Name:</label>
    <input type="text" name="name" class="form-control" />
    <label>Phone Number:</label>
    <input type="text" name="phno" class="form-control" />
    <honeypot name="email" class="form-control fs"></honeypot>
    <label>Age :</label>
    <input type="text" name="age" class="form-control" />
    <input type="submit" class="btn btn-light" value="Register" />
</form>
